{
  "name": "nacomer_react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1",
    "react-scripts": "4.0.0",
    "react-tabs": "^3.1.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier",
    "lint": "prettier --write . && eslint --fix 'src/**/*.jsx' 'src/**/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "globals": {
      "window": true,
      "document": true
    },
    "rules": {
      "import/prefer-default-export": 0,
      "react/prop-types": 0,
      "react/destructuring-assignment": 0,
      "react/no-array-index-key": 0,
      "no-unused-vars": 0,
      "no-shadow": 0,
      "no-alert": 0,
      "jsx-a11y/alt-text": 0,
      "jsx-a11y/iframe-has-title": 0,
      "react/button-has-type": 0,
      "max-len": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "prettier-eslint": "^12.0.0"
  },
  "lint-staged": {
    "*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
